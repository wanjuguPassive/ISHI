	<script>

		$(document).ready(function() {

			function fluid() {

				var bWidth = $(window).width();
				var bHeight = $(window).height();

				if ( bHeight >= 915 ) {
					var adjHeight = bHeight * .83;
				} else if ( bHeight < 915 && bHeight >= 750 ) {
					var adjHeight = bHeight * .80;
				} else if ( bHeight < 750 && bHeight >= 600 ) {
					var adjHeight = bHeight * .75;
				} else if ( bHeight < 600 ) {
					var adjHeight = bHeight * .69;
				}
				$('.scroll .photo').height(adjHeight);
				$('.scroll iframe').height(adjHeight);

				$('.scroll iframe').each(function() {

					var theVideo = $(this);
					var theHeight = adjHeight;
					videoSizeHeight(theVideo, theHeight, "no");

				});

			}

			fluid();

			$(window).resize(function() {

				fluid();
				scroller();

			});

		});

		function videoSizeHeight(theVideo, theHeight, ignoreWidth) {

			var vidW = theVideo.attr('width');
			var vidH = theVideo.attr('height');
			var vidRatio = vidH / vidW;

			var newWidth = theHeight / vidRatio;

			if (ignoreWidth === "no") {
				theVideo.width(newWidth).height(theHeight);
			} else {
				theVideo.height(theHeight);
			}

		}

		var scrollOptions = {
		  fluidClass: 'imgix-fluid-scroll',
		  updateOnResizeDown: true,
		  updateOnPinchZoom: true,
		  pixelStep: 10,
		  autoInsertCSSBestPractices: true
		}

		/* LFET RIGHT ARROW CLICKS */
		$('#next').click(function() {
			$('.swipeCallout').fadeTo(300,0);
			var currentImg = $('body').data('imageview');
			var nextImg = currentImg + 1;
			var theLeft = ($('#'+nextImg).offset().left - 59);
			$('body').scrollTo(theLeft, 300, {
				axis:'x',
				onAfter:function() {

					$('body').data('imageview', nextImg);
				}

			});

		});

		$('#prev').click(function() {
			$('.swipeCallout').fadeTo(300,0);
			var currentImg = $('body').data('imageview');
			var nextImg = currentImg - 1;
			var theLeft = ($('#'+nextImg).offset().left - 59);
			$('body').scrollTo(theLeft, 300, {
				axis:'x',
				onAfter:function() {

					$('body').data('imageview', nextImg);
				}

			});

		});

		function scroller() {

			var theHash = window.location.hash;
			var cleanHash = parseInt(theHash.replace("#img", ""));
			console.log(cleanHash);
			$('body').data('imageview', cleanHash);
			var theLeft = ($('#'+cleanHash).offset().left - 59);

			$('body').scrollTo(theLeft, 300, {
				axis:'x',
				onAfter:function() {

					var realImg = $('#'+cleanHash).find('.photo').data('src');
					$('#'+cleanHash).find('.photo').attr('src', realImg);

					imgix.fluid(scrollOptions);
				}

			});

		}

		$(window).load(function() {

			scroller();

		});

	</script>

